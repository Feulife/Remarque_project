d":{"line":63,"column":1},"source":"components/Remarque.js","original":{"line":30,"column":0}},{"name":"Remarque","generated":{"line":65,"column":15},"source":"components/Remarque.js","original":{"line":64,"column":15}}],"sources":{"components/Remarque.js":"import React from 'react';\nimport ReactMarkdown from 'react-markdown';\nimport { format } from 'date-fns';\nimport styled from 'styled-components';\nimport { useQuery } from '@apollo/client';\n\nimport RemarqueUser from './RemarqueUser';\nimport { IS_LOGGED_IN } from '../gql/query';\n\nconst StyledRemarque = styled.article`\n  max-width: 800px;\n  margin: 0 auto;\n`;\n\nconst MetaData = styled.div`\n  @media (min-width: 500px) {\n    display: flex;\n    align-items: top;\n  }\n`;\n\nconst MetaInfo = styled.div`\n  padding-right: 1em;\n`;\n\nconst UserActions = styled.div`\n  margin-left: auto;\n`;\n\nconst Remarque = ({ remarque }) => {\n  const { loading, error, data } = useQuery(IS_LOGGED_IN);\n  if (loading) return <p>Loading...</p>;\n  if (error) return <p>Error!</p>;\n\n  return (\n    <StyledRemarque>\n      <MetaData>\n        <MetaInfo>\n          <img\n            src={remarque.author.avatar}\n            alt={`${remarque.author.username} avatar`}\n            height=\"50px\"\n          />\n        </MetaInfo>\n        <MetaInfo>\n          <em>by</em> {remarque.author.username} <br />\n          {format(remarque.createdAt, 'MMM Do YYYY')}\n        </MetaInfo>\n        {data.isLoggedIn ? (\n          <UserActions>\n            <NoteUser note={remarque} />\n          </UserActions>\n        ) : (\n          <UserActions>\n            <em>Favorites:</em> {remarque.favoriteCount}\n          </UserActions>\n        )}\n      </MetaData>\n      <ReactMarkdown source={remarque.content} />\n    </StyledRemarque>\n  );\n};\n\nexport default Remarque;"},"lineCount":null}},"error":null,"hash":"4d79978116c014b3cc1ff649b53bacd0","cacheData":{"env":{}}}