:"pages/index.js","original":{"line":53,"column":21}},{"generated":{"line":100,"column":0},"source":"pages/index.js","original":{"line":51,"column":16}},{"generated":{"line":101,"column":0},"source":"pages/index.js","original":{"line":50,"column":10}},{"generated":{"line":101,"column":8},"source":"pages/index.js","original":{"line":46,"column":19}},{"generated":{"line":102,"column":0},"source":"pages/index.js","original":{"line":46,"column":19}},{"generated":{"line":103,"column":0},"source":"pages/index.js","original":{"line":44,"column":4}},{"generated":{"line":103,"column":5},"source":"pages/index.js","original":{"line":43,"column":2}},{"generated":{"line":104,"column":0},"source":"pages/index.js","original":{"line":60,"column":1}},{"generated":{"line":104,"column":1},"source":"pages/index.js","original":{"line":39,"column":0}},{"name":"Pages","generated":{"line":106,"column":15},"source":"pages/index.js","original":{"line":62,"column":15}}],"sources":{"pages/index.js":"import React from 'react';\nimport { useQuery, gql } from '@apollo/client';\nimport { BrowserRouter as Router, Route, Redirect } from 'react-router-dom';\n\nimport Layout from '../components/Layout';\n\nimport Home from './home';\nimport MyRemarques from './myremarques';\nimport Favorites from './favorites';\nimport Remarque from './remarque';\nimport SignUp from './signup';\nimport SignIn from './signin';\nimport NewRemarque from './new';\nimport EditRemarque from './edit';\n\nconst IS_LOGGED_IN = gql`\n  {\n    isLoggedIn @client\n  }\n`;\n\nconst Pages = () => {\n  return (\n    <Router>\n      <Layout>\n        <Route exact path=\"/\" component={Home} />\n        <PrivateRoute path=\"/myremarques\" component={MyRemarques} />\n        <PrivateRoute path=\"/favorites\" component={Favorites} />\n        <Route path=\"/remarque/:id\" component={Remarque} />\n        <Route path=\"/signup\" component={SignUp} />\n        <Route path=\"/signin\" component={SignIn} />\n        <PrivateRoute path=\"/new\" component={NewRemarque} />\n        <PrivateRoute path=\"/edit/:id\" component={EditRemarque} />\n      </Layout>\n    </Router>\n  );\n};\n\nconst PrivateRoute = ({ component: Component, ...rest }) => {\n  const { loading, error, data } = useQuery(IS_LOGGED_IN);\n  if (loading) return <p>Loading...</p>;\n  if (error) return <p>Error!</p>;\n  return (\n    <Route\n      {...rest}\n      render={props =>\n        data.isLoggedIn === true ? (\n          <Component {...props} />\n        ) : (\n          <Redirect\n            to={{\n              pathname: '/signin',\n              state: { from: props.location }\n            }}\n          />\n        )\n      }\n    />\n  );\n};\n\nexport default Pages;"},"lineCount":null}},"error":null,"hash":"5df95ace25a6bbf02cce574944bb635d","cacheData":{"env":{}}}